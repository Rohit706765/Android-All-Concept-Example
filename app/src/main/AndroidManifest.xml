        <activity android:name=".Scrollview.ScrollViewConcept" />
        <activity android:name=".GridView.GridViewConcept" />
        <activity android:name=".BaseAdapter.ListViewAdapter" />
        <activity android:name=".BaseAdapter.BaseAdapterConcept" />
        <activity android:name=".FrameLayout.FrameLayout" />
        <activity android:name=".TableLayout.TableLayout_concept" />
        <activity android:name=".RelativeLayout.RelativeLayoutConcept" />
